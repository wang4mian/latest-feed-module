{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "astro",
  "functions": {
    "api/rss-cron.js": {
      "runtime": "nodejs20.x"
    },
    "api/job-processor-cron.js": {
      "runtime": "nodejs20.x"
    }
  },
  "crons": [
    {
      "path": "/api/rss-cron",
      "schedule": "0 */2 * * *"
    },
    {
      "path": "/api/job-processor-cron",
      "schedule": "*/15 * * * *"
    }
  ]
}